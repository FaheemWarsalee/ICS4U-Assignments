class piDayTest
{
  public static void main(String[] args)
  {
    int pcs = 8;
    int ppl = 4;
    int max;
    
    if(pcs % ppl == 0)
      max = pcs/ppl;
    else
      max = 1;
    
    System.out.println(countWays(pcs,ppl, max));
  }
  
  public static int countWays(int pcs, int ppl,int max) 
  { 
    int counter = 0; // Initialize result 
    return counter;
  } 
}

